{
	"name": "bootstrap-vue",
	"displayName": "Bootstrap Vue Extension",
	"description": "Bootstrap Vue Extension",
	"repository": { "type": "git", "url": "https://github.com/jd-0001/vs-code-extension-bootstrap-vue.git" },
	"version": "0.0.1",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
