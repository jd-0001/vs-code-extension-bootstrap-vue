{
  "b-button": {
    "prefix": "btn",
    "body": ["<b-button${1: variant=\"${2:primary}\"}>${3:Text}</b-button>"],
    "description": "BV: b-button"
  },
  "b-button Outline": {
    "prefix": "btn-outline",
    "body": ["<b-button variant=\"outline-${1:primary}\">${3:Text}</b-button>"],
    "description": "BV: b-button Outline"
  },
  "b-button Pill": {
    "prefix": "btn-pill",
    "body": ["<b-button pill${1: variant=\"${2:primary}\"}>${3:Text}</b-button>"],
    "description": "BV: b-button Pill"
  },
  "b-button Square": {
    "prefix": "btn-square",
    "body": ["<b-button squared${1: variant=\"${2:primary}\"}>${3:Text}</b-button>"],
    "description": "BV: b-button Squared"
  },
  "b-button Size": {
    "prefix": "btn-size",
    "body": ["<b-button size=\"${1:sm}\"${2: variant=\"${3:primary}\"}>${4:Text}</b-button>"],
    "description": "BV: b-button Size"
  },
  "Alert": {
    "prefix": "alert",
    "body": ["<b-alert variant=\"${1:primary}\"${2: show}>${3:string}</b-alert>"],
    "description": "BV: Alert"
  },
  "Alert Dismissible": {
    "prefix": "alert-dis",
    "body": ["<b-alert variant=\"${1:primary}\"${2: show}${3: dismissible}>${4:string}</b-alert>"],
    "description": "BV: Alert Dismissible"
  },
  "Alert v-model": {
    "prefix": "alert-v-model",
    "body": ["<b-alert v-model=\"${1:model}\" variant=\"${2:primary}\">${3:string}</b-alert>"],
    "description": "BV: Alert v-model"
  }
}
