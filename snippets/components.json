{
  "b-button": {
    "prefix": "btn",
    "body": ["<b-button${1: variant=\"${2:primary}\"}>${3:Text}</b-button>"],
    "description": "BV: b-button"
  },
  "b-button Outline": {
    "prefix": "btn-outline",
    "body": ["<b-button variant=\"outline-${1:primary}\">${3:Text}</b-button>"],
    "description": "BV: b-button Outline"
  },
  "b-button Pill": {
    "prefix": "btn-pill",
    "body": ["<b-button pill${1: variant=\"${2:primary}\"}>${3:Text}</b-button>"],
    "description": "BV: b-button Pill"
  },
  "b-button Square": {
    "prefix": "btn-square",
    "body": ["<b-button squared${1: variant=\"${2:primary}\"}>${3:Text}</b-button>"],
    "description": "BV: b-button Squared"
  },
  "b-button Size": {
    "prefix": "btn-size",
    "body": ["<b-button size=\"${1:sm}\"${2: variant=\"${3:primary}\"}>${4:Text}</b-button>"],
    "description": "BV: b-button Size"
  },
  "Alert": {
    "prefix": "alert",
    "body": ["<b-alert variant=\"${1:primary}\"${2: show}>${3:string}</b-alert>"],
    "description": "BV: Alert"
  },
  "Alert Dismissible": {
    "prefix": "alert-dis",
    "body": ["<b-alert variant=\"${1:primary}\"${2: show}${3: dismissible}>${4:string}</b-alert>"],
    "description": "BV: Alert Dismissible"
  },
  "Alert v-model": {
    "prefix": "alert-v-model",
    "body": ["<b-alert v-model=\"${1:model}\" variant=\"${2:primary}\">${3:string}</b-alert>"],
    "description": "BV: Alert v-model"
  },
  "BV: Avatar": {
    "prefix": "avatar",
    "body": ["<b-avatar variant=\"${1:primary}\">${2:string}</b-avatar>"],
    "description": "BV: Avatar"
  },
  "BV: Avatar with Size": {
    "prefix": "avatar-with-size",
    "body": ["<b-avatar size=\"${1:sm}\" variant=\"${2:primary}\">${3:string}</b-avatar>"],
    "description": "BV: Avatar with Size"
  },
  "BV: Avatar Text": {
    "prefix": "avatar-text",
    "body": ["<b-avatar variant=\"${1:primary}\" text=\"${2:text}\" />"],
    "description": "BV: Avatar Text"
  },
  "BV: Avatar Text with Size": {
    "prefix": "avatar-text-with-size",
    "body": ["<b-avatar size=\"${1:sm}\" variant=\"${2:primary}\" text=\"${3:text}\" />"],
    "description": "BV: Avatar Text with Size"
  },
  "BV: Avatar src": {
    "prefix": "avatar-src",
    "body": ["<b-avatar variant=\"${1:primary}\" src=\"${2:src}\" />"],
    "description": "BV: Avatar src"
  },
  "BV: Avatar src with Size": {
    "prefix": "avatar-src-with-size",
    "body": ["<b-avatar size=\"${1:sm}\"  variant=\"${2:primary}\" src=\"${3:src}\" />"],
    "description": "BV: Avatar src with Size"
  },
  "BV: Avatar Icon": {
    "prefix": "avatar-icon",
    "body": ["<b-avatar variant=\"${1:primary}\" icon=\"${2:icon}\" />"],
    "description": "BV: Avatar Icon"
  },
  "BV: Avatar Icon with Size": {
    "prefix": "avatar-icon-with-size",
    "body": ["<b-avatar size=\"${1:sm}\" variant=\"${2:primary}\" icon=\"${3:icon}\" />"],
    "description": "BV: Avatar Icon with Size"
  },
  "BV: Badge": {
    "prefix": "badge",
    "body": ["<b-badge variant=\"${1:primary}\">${2:string}</b-badge>"],
    "description": "BV: Badge"
  },
  "BV: Badge Pill": {
    "prefix": "badge-pill",
    "body": ["<b-badge pill variant=\"${1:primary}\">${2:string}</b-badge>"],
    "description": "BV: Badge Pill"
  },
  "BV: Button Group": {
    "prefix": "btn-grp",
    "body": ["<b-button-group variant=\"${1:primary}\">", "  ${2}", "</b-button-group>"],
    "description": "BV: Button Group"
  },
  "BV: Button Group with Size": {
    "prefix": "btn-grp-with-size",
    "body": ["<b-button-group size=\"${1:sm}\" variant=\"${2:primary}\">", "  ${3}", "</b-button-group>"],
    "description": "BV: Button Group with Size"
  },
  "BV: Card": {
    "prefix": "card",
    "body": ["<b-card title=\"${1:title}\">", "  ${2}", "</b-card>"],
    "description": "BV: Card"
  },
  "BV: Card with Sub-title": {
    "prefix": "card-with-sub-title",
    "body": ["<b-card title=\"${1:title}\" sub-title=\"${2:sub-title}\">", "  ${3}", "</b-card>"],
    "description": "BV: Card with Sub-title"
  },
  "BV: Card with Image": {
    "prefix": "card-with-img",
    "body": [
      "<b-card title=\"${1:title}\" img-src=\"${2:src}\" img-alt=\"${3:alt-text}\"${4: img-top}>",
      "  ${5}",
      "</b-card>"
    ],
    "description": "BV: Card with Image"
  },
  "BV: Dropdown": {
    "prefix": "dropdown",
    "body": ["<b-dropdown variant=\"${1:primary}\" text=\"${2:dropdown-text}\">", "  ${3}", "</b-dropdown>"],
    "description": "BV: Dropdown"
  },
  "BV: Dropdown with Size": {
    "prefix": "dropdown-with-size",
    "body": [
      "<b-dropdown size=\"${1:sm}\" variant=\"${2:primary}\" text=\"${3:dropdown-text}\">",
      "  ${4}",
      "</b-dropdown>"
    ],
    "description": "BV: Dropdown with Size"
  },
  "BV: Form": {
    "prefix": "form",
    "body": ["<b-form @submit=\"${1:submit_method}\"${2: @reset=\"${3:reset_method}\"}>", "  ${4}", "</b-form>"],
    "description": "BV: Form"
  },
  "BV: Form Inline": {
    "prefix": "form-inline",
    "body": ["<b-form @submit=\"${1:submit_method}\"${2: @reset=\"${3:reset_method}\"} inline>", "  ${4}", "</b-form>"],
    "description": "BV: Form Inline"
  },
  "BV: Form Group": {
    "prefix": "form-grp",
    "body": [
      "<b-form-group${1: id=\"${2:id}\"}${3: label=\"${4:label}\" label-for=\"${5:for}\"}>",
      "  ${6}",
      "</b-form-group>"
    ],
    "description": "BV: Form Group"
  }
}
